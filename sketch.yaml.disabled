profiles:
  profile-MEGA-COM6:
    fqbn: arduino:avr:mega:cpu=atmega2560
    platforms:
      - platform: arduino:avr (1.8.6)
    programmer: ""
    port: COM6
    port_config:
      baudrate: "115200"
      lineEnding: "\r\n"
      bits: "8"
      parity: none
      stop_bits: one
default_profile: profile-MEGA-COM6
